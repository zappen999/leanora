  "repository": {
    "type": "git",
    "url": "git+https://github.com/zappen999/leanora.git"
  },
  "keywords": [
    "Docker",
    "Nodejs",
    "Microservice"
  ],
  "author": "Johan Kanefur <johan.canefur@gmail.com>",
  "contributors": "Joel Roxell <joel.roxell@annevo.se> (https://github.com/JoelRoxell)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zappen999/leanora/issues"
  },
  "homepage": "https://github.com/zappen999/leanora#readme",
  "dependencies": {
    "@types/bcrypt": "^1.0.0",
    "@types/enzyme": "^3.1.5",
    "@types/graphql": "^0.11.7",
    "@types/jest": "^21.1.8",
    "@types/jsonwebtoken": "^7.2.3",
    "@types/koa": "^2.0.40",
    "@types/koa-bodyparser": "^3.0.26",
    "@types/koa-jwt": "^3.2.2",
    "@types/koa-mount": "^2.0.19",
    "@types/koa-router": "^7.0.25",
    "@types/koa-static": "^3.0.2",
    "@types/node": "^8.0.54",
    "@types/react": "^16.0.26",
    "@types/react-dom": "^16.0.3",
    "@types/react-redux": "^5.0.14",
    "@types/winston": "^2.3.7",
    "apollo-link": "^1.0.5",
    "apollo-server-koa": "^1.2.0",
    "autoprefixer": "7.1.2",
    "bcrypt": "^1.0.3",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "css-loader": "0.28.4",
    "dataloader": "^1.3.0",
    "dotenv": "4.0.0",
    "extract-text-webpack-plugin": "3.0.0",
    "file-loader": "0.11.2",
    "fs-extra": "3.0.1",
    "graphql": "^0.11.7",
    "graphql-iso-date": "^3.3.0",
    "graphql-tools": "^2.7.2",
    "graphql-type-json": "^0.1.4",
    "html-webpack-plugin": "2.29.0",
    "jest": "20.0.4",
    "jsonwebtoken": "^8.1.0",
    "koa": "^2.4.1",
    "koa-bodyparser": "^4.2.0",
    "koa-mount": "^3.0.0",
    "koa-router": "^7.3.0",
    "koa-static": "^4.0.2",
    "mysql": "^2.15.0",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.6",
    "promise": "8.0.1",
    "react": "^16.2.0",
    "react-dev-utils": "^4.0.1",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "reflect-metadata": "^0.1.10",
    "source-map-loader": "^0.2.1",
    "style-loader": "0.18.2",
    "sw-precache-webpack-plugin": "0.11.4",
    "ts-jest": "^20.0.7",
    "ts-loader": "^2.3.7",
    "typeorm": "^0.1.7",
    "typescript": "^2.6.1",
    "url-loader": "0.5.9",
    "webpack": "3.5.1",
    "webpack-dev-server": "2.7.1",
    "webpack-manifest-plugin": "1.2.1",
    "whatwg-fetch": "2.0.3",
    "winston": "^2.4.0"
  },
  "devDependencies": {
    "husky": "^0.11.9",
    "nodemon": "^1.12.1",
    "ts-node": "^3.2.0",
    "tslint": "^5.8.0",
    "tslint-loader": "^3.5.3",
    "tslint-react": "^3.2.0",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "node-sass": "^4.7.2",
    "react-addons-test-utils": "^15.6.2",
    "react-test-renderer": "^16.2.0",
    "sass-loader": "^6.0.6",
    "typings-for-css-modules-loader": "^1.7.0"
  },
  "scripts": {
    "start-app": "node src/app/scripts/start.js",
    "build-app": "node src/app/scripts/build.js",
    "test-app": "node src/app/scripts/test.js --env=jsdom",
    "precommit": "./node_modules/.bin/tslint -c tslint.json 'src/**/*.{ts,tsx}'",
    "bash": "docker exec -it $(./scripts/containername.sh) bash",
    "typeorm": "docker exec -it $(./scripts/containername.sh) ./node_modules/.bin/ts-node ./node_modules/.bin/typeorm",
    "migrations:up": "docker exec -it $(./scripts/containername.sh) ./node_modules/.bin/ts-node ./node_modules/.bin/typeorm migrations:run",
    "migrations:down": "docker exec -it $(./scripts/containername.sh) ./node_modules/.bin/ts-node ./node_modules/.bin/typeorm migrations:revert"
  },
  "jest": {
    "mapCoverage": true,
    "collectCoverageFrom": [
      "<rootDir>/src/app/src/**/*.{js,jsx,ts,tsx}"
    ],
    "setupFiles": [
      "<rootDir>/src/app/config/polyfills.js",
      "<rootDir>/src/app/config/enzyme-adapter.js"
    ],
    "testMatch": [
      "<rootDir>/src/app/src/**/__tests__/**/*.ts?(x)",
      "<rootDir>/src/app/src/**/?(*.)(spec|test).ts?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.css$": "<rootDir>/src/app/config/jest/cssTransform.js",
      "^.+\\.tsx?$": "<rootDir>/src/app/config/jest/typescriptTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/src/app/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "web.js",
      "js",
      "web.jsx",
      "jsx",
      "json",
      "node"
    ],
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.test.json"
      }
    }
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "leanConfig": {
    "entires": {
      "app": "src/app/"
    }
  }
}
